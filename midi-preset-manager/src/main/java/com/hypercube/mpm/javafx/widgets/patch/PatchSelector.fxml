<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<fx:root type="GridPane" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        <RowConstraints/>
    </rowConstraints>
    <children>
        <Button mnemonicParsing="false" text="Patches" GridPane.halignment="CENTER" GridPane.rowIndex="0"/>
        <ListView fx:id="patchList" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS"
                  items="${controller.model.root.patches}"/>
    </children>
</fx:root>
