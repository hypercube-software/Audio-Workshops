                           GridPane.vgrow="ALWAYS"/>
        <AttributeSelector minWidth="-Infinity" GridPane.columnIndex="2" GridPane.hgrow="NEVER"
                           GridPane.vgrow="ALWAYS"/>
        <PatchSelector minWidth="-Infinity" GridPane.columnIndex="3" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS"/>
    </children>
    <columnConstraints>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
</fx:root>
