        <AttributeSelector minHeight="-Infinity" minWidth="-Infinity" GridPane.columnIndex="0" GridPane.hgrow="NEVER"
                           GridPane.vgrow="ALWAYS"
                           dataSource="controller.model.root.devices"
        />
